  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=5096 node_modules/@angular/cli/bin/ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "builddevelopment": "npm run memory && ng build --configuration=development --aot=false --build-optimizer=false",
    "buildproduction": "npm run memory && ng build --configuration=production --aot=false --build-optimizer=false && npm run  postbuildprod",
    "add-page": "ng g m pages/page-name --routing && ng g component pages/page-name --skip-tests",
    "memory": "export NODE_OPTIONS='--max-old-space-size=4096'",
    "postbuildprod": "sudo cp -R -p /var/www/alexa-design.mijnperceel.be/mijnperceel/dist/mijnperceel/. /var/www/makelaars.mijnperceel.be/"
  },
